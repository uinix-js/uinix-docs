{
  "private": true,
  "name": "uinix-docs",
  "version": "1.0.0",
  "description": "Progressive docs for the uinix ecosystem",
  "license": "MIT",
  "keywords": [
    "uinix",
    "docs",
    "ui",
    "ui-system"
  ],
  "repository": "uinix-js/uinix-docs",
  "bugs": "https://github.com/uinix-js/uinix-docs/issues",
  "author": "Chris Zhou <chrisrzhou@pm.me> (https://chrisrzhou.io)",
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^3.8.0",
    "gatsby-plugin-mdx": "^2.8.0",
    "gatsby-remark-autolink-headers": "^4.5.0",
    "gatsby-remark-table-of-contents": "^1.0.0",
    "gatsby-source-filesystem": "^3.8.0",
    "github-markdown-css": "^4.0.0",
    "palx": "^1.0.3",
    "prism-react-renderer": "^1.2.1",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-live": "^2.2.3",
    "remark": "^13.0.0",
    "remark-highlight.js": "^6.0.0",
    "remark-react": "^8.0.0",
    "remark-slug": "^6.0.0",
    "uinix-ui": "^0.1.5-beta.1"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.25.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "xo": "^0.40.3"
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "lint": "xo --fix",
    "serve": "gatsby serve",
    "start": "gatsby develop"
  },
  "prettier": {
    "bracketSpacing": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "xo": {
    "esnext": false,
    "env": [
      "browser"
    ],
    "extends": [
      "xo-react"
    ],
    "prettier": true,
    "rules": {
      "react/no-array-index-key": "off",
      "react/prop-types": "off",
      "unicorn/no-array-callback-reference": "off",
      "unicorn/no-array-for-each": "off",
      "unicorn/no-array-reduce": "off"
    },
    "overrides": [
      {
        "files": [
          "gatsby-*.js"
        ],
        "rules": {
          "unicorn/prefer-module": "off",
          "unicorn/prefer-node-protocol": "off"
        }
      }
    ]
  }
}
